import re
#1 вариант
new=re.findall(r'[a-z]+(?=[A-Z]+)',line)
print(new)
#2 вариант
new=[]
i=""
for l in range(len(line)):
    if line[l].islower():
        i+=line[l]
    elif line[l].isupper() and line[l-1].islower():
        new.append(i)
        i=""      
print(new)
    


        
