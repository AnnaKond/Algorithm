# Вариант 1 с помощью re
import re
new=re.findall(r'(?<=[a-z]{2})([A-Z]{1,}(?=[A-Z]{2}))',line)
print(new)
# Вариант 2 без помощи re
new=[]
i=""
for l in range(len(line)-2):
    if line[l].isupper() and line[l-1].islower()and line[l-2].islower() and line[l+1].isupper() and line[l+2].isupper():
        i+=line[l]
    elif len(i)>0 and line[l].isupper() and line[l-1]==i[len(i)-1] and line[l+1].isupper()and line[l+2].isupper():
        i+=line[l]
    elif len(i)>0 and line[l].isupper() and line[l-1]==i[len(i)-1] and line[l+1].isupper() and line[l+2].lower():
        new.append(i)
        i=""        
print(new)


        
